    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@umijs/preset-react": "1.x",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
