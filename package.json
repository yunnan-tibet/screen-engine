    "@scena/react-guides": "^0.28.2",
    "@types/html2canvas": "0.0.35",
    "@types/react-color": "^3.0.4",
    "antd": "^5.16.0",
    "classnames": "^2.5.1",
    "dva-model-persist": "^1.0.0",
    "echarts": "^5.5.0",
