      "eslint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "prettier --parser --write"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
